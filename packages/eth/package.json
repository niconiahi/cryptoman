{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@ethernauta/eth",
  "type": "module",
  "version": "0.0.10",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/niconiahi/cryptoman"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown && biome format --write dist",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@ethernauta/transport": "workspace:*",
    "@ethernauta/chain": "workspace:*",
    "@noble/hashes": "^1.8.0",
    "@noble/secp256k1": "^2.3.0",
    "@scure/bip32": "^1.7.0",
    "@scure/bip39": "^1.6.0"
  },
  "peerDependencies": {
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "tsdown": "^0.13.3"
  }
}
