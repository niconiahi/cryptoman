{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@ethernauta/transport",
  "type": "module",
  "version": "0.0.16",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/niconiahi/cryptoman"
  },
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown && biome format --write dist",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@ethernauta/eth": "workspace:*",
    "@ethernauta/chain": "workspace:*"
  },
  "peerDependencies": {
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.1",
    "tsdown": "^0.13.3"
  }
}
